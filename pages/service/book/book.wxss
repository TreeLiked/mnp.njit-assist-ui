
.header {
  position: fixed;
  z-index: 99;
  background-color: #FFFAFA;
  border-radius: 0 0 10px 10px;
  width: 100%;
}

.mycard-container {
  margin-top: 100px;
}

.item-container {
  border-radius: 200px;
  margin: 5px 15px;
}

.item-row {
  padding-top: 5px;
}

.item-label {
  color: #828282;
  font-size: 28rpx;
}

.item-value {
  color: #1C1C1C;
  font-size: 28rpx;
}

.text {
  height: 30rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
  line-height: 30rpx;
  margin-top: 9rpx;
}

