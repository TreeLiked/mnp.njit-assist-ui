

.item-container {
  border-radius: 200px;
  margin: 0px 5px;

}

.item-row {
  padding: 0px 10px;
}

.item-label {
  color: #828282;
  font-size: 26rpx;
}

.item-value {
  color: #1C1C1C;
  font-size: 26rpx;
}

.content-card {
  border-bottom: 20rpx solid rgb(173, 27, 27) !important;
  border-radius: 200rpx !important;
}

.text {
  height: 30rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
  line-height: 30rpx;
  margin-top: 9rpx;
}

